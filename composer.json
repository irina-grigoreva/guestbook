{
    "autoload": {
        "psr-4": {
            "core\\": "vendor/core",
            "app\\": "src/"
        }
    },
    "require": {
        "gabordemooij/redbean": "^5.7",
        "altorouter/altorouter": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.13"
    },
    "scripts": {
        "cs": "phpcs -p --standard=PSR12 --extensions=php src",
        "cs:fix": "phpcbf -p --standard=PSR12 --extensions=php src"
    }
}
